{
    "name": "secalith/array-digger",
    "description": "Extracts the value from given array by given path (with delimiter). as default delimiter it uses dot(.)",
    "type": "library",
    "license": "BSD-3-Clause",
    "version": "stable",
    "authors": [
        {
            "name": "Jan F Kowalski",
            "email": "jan@secalith.co.uk"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0"
    },
    "psr-4": {
        "Secalith\\ArrayDigger\\": "src/"
    },
    "extra": {
        "zf": {
            "config-provider": "Secalith\\ArrayDigger\\ConfigProvider"
        },
        "branch-alias": {
          "stable": "1.x-dev",
          "dev-master": "1.x-dev",
          "dev-develop": "1.x-dev"
        }
    }
}
